digraph symmetry {

  # a 'graph' statement
  graph [overlap = false, fontsize = 10, rankdir = LR]

  # several 'node' statements
  node [shape = circle,
        fixedsize = true,
        width = 0.5,
        style = filled,
        color = grey]
  node [fillcolor = PowderBlue]
  A; B; C

  # several 'edge' statements
  A->B A->C C->B
}
